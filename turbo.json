{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**",
        ".angular/**",
        "build/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "test": {},
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
